{
  "name": "Ranger",
  "sections": [
    {
      "header": "",
      "items": [
        {
          "name": "",
          "text": "As a ranger, you gain the following Class Features."
        }
      ]
    },
    {
      "header": "Hit Points",
      "level": "2",
      "items": [
        {
          "name": "",
          "text": ""
        },
        {
          "name": "Hit Dice",
          "text": "1d10 per ranger level"
        },
        {
          "name": "Hit Points at 1st Level",
          "text": "10 + your Constitution modifier"
        },
        {
          "name": "Hit Points at Higher Levels",
          "text": "1d10 (or 6) + your Constitution modifier per ranger level after 1st"
        }
      ]
    },
    {
      "header": "Starting Proficiencies",
      "level": "2",
      "items": [
        {
          "name": "",
          "text": "You are proficient with the following items, in addition to any proficiencies provided by your race or background."
        },
        {
          "name": "Armor",
          "text": "Light Armor, Medium Armor, shields"
        },
        {
          "name": "Weapons",
          "text": "simple Weapons, martial Weapons"
        },
        {
          "name": "Tools",
          "text": "none"
        },
        {
          "name": "Skills",
          "text": "Choose three from Animal Handling, Athletics, Insight, Investigation, Nature, Perception, Stealth, and Survival"
        }
      ]
    },
    {
      "header": "Starting Equipment",
      "level": "2",
      "items": [
        {
          "name": "",
          "text": ""
        },
        {
          "name": "",
          "text": "You start with the following items, plus anything provided by your background."
        },
        {
          "name": "",
          "text": "• (a) Scale Mail or (b) Leather Armor"
        },
        {
          "name": "",
          "text": "• (a) two shortswords or (b) two simple Melee Weapons"
        },
        {
          "name": "",
          "text": "• (a) a Dungeoneer's Pack or (b) an Explorer's Pack"
        },
        {
          "name": "",
          "text": "• A Longbow and a Quiver of 20 Arrows"
        },
        {
          "name": "",
          "text": "Alternatively, you may start with 5d4 x 10 gp to buy your own Equipment."
        }
      ]
    },
