{
  "name": "Monk",
  "sections": [
    {
      "header": "",
      "items": [
        {
          "name": "",
          "text": "As a monk, you gain the following Class Features."
        }
      ]
    },
    {
      "header": "Hit Points",
      "level": "2",
      "items": [
        {
          "name": "",
          "text": ""
        },
        {
          "name": "Hit Dice",
          "text": "1d8 per monk level"
        },
        {
          "name": "Hit Points at 1st Level",
          "text": "8 + your Constitution modifier"
        },
        {
          "name": "Hit Points at Higher Levels",
          "text": "1d8 (or 5) + your Constitution modifier per monk level after 1st"
        }
      ]
    },
    {
      "header": "Starting Proficiencies",
      "level": "2",
      "items": [
        {
          "name": "",
          "text": "You are proficient with the following items, in addition to any proficiencies provided by your race or background."
        },
        {
          "name": "Armor",
          "text": "none"
        },
        {
          "name": "Weapons",
          "text": "simple Weapons, shortswords"
        },
        {
          "name": "Tools",
          "text": "any one type of artisan's tools or any one musical instrument of your choice"
        },
        {
          "name": "Skills",
          "text": "Choose two from Acrobatics, Athletics, History, Insight, Religion, and Stealth"
        }
      ]
    },
    {
      "header": "Starting Equipment",
      "level": "2",
      "items": [
        {
          "name": "",
          "text": ""
        },
        {
          "name": "",
          "text": "You start with the following items, plus anything provided by your background."
        },
        {
          "name": "",
          "text": "• (a) a Shortsword or (b) any simple weapon"
        },
        {
          "name": "",
          "text": "• (a) a Dungeoneer's Pack or (b) an Explorer's Pack"
        },
        {
          "name": "",
          "text": "• 10 darts"
        },
        {
          "name": "",
          "text": "Alternatively, you may start with 5d4 gp to buy your own Equipment."
        }
      ]
    },
